{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "./prisma/**/*"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}